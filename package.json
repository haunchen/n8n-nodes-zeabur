{
	"name": "n8n-nodes-zeabur",
	"version": "0.1.0",
	"description": "n8n community node for Zeabur AI Hub integration, supporting OpenAI, Claude, Gemini and more AI models",
	"license": "MIT",
	"homepage": "https://github.com/zeabur/n8n-nodes-zeabur",
	"keywords": [
		"n8n-community-node-package",
		"zeabur",
		"ai-hub",
		"openai",
		"claude",
		"gemini",
		"llm",
		"ai",
		"chatgpt"
	],
	"author": {
		"name": "Zeabur",
		"email": "support@zeabur.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/zeabur/n8n-nodes-zeabur.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/ZeaburAIHubApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/LmChatZeaburAiHub/LmChatZeaburAiHub.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"@types/node": "^24.10.1",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"@langchain/openai": "^1.1.3",
		"n8n-workflow": "*"
	}
}