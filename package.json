{
	"name": "@haunchen/n8n-nodes-zeabur",
	"version": "0.3.1",
	"description": "n8n community node for Zeabur AI Hub integration, supporting OpenAI, Claude, Gemini and more AI models",
	"publishConfig": {
		"access": "public"
	},
	"license": "MIT",
	"homepage": "https://github.com/haunchen/n8n-nodes-zeabur",
	"keywords": [
		"n8n-community-node-package",
		"zeabur",
		"ai-hub",
		"openai",
		"claude",
		"gemini",
		"llm",
		"ai",
		"chatgpt"
	],
	"author": {
		"name": "Frank Chen",
		"email": "frank@frankchen.tw"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/haunchen/n8n-nodes-zeabur.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/ZeaburAIHubApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/LmChatZeaburAiHub/LmChatZeaburAiHub.node.js",
			"dist/nodes/ZeaburAiHub/ZeaburAiHub.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"@types/node": "^24.10.1",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
